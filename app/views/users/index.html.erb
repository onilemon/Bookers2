<%= render 'list', user: @user %>

<h2>Users</h2>
  <table>
    <thead>
        <tr>
            <th>image</th>
           <th>name</th>
        </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= image_tag @user.get_profile_image(100,100) %>
            <%= @user.name %>
            <%= link_to "Show", user_path(@user.id) %>
          </td>
      </tr>
    </tbody>
  </table>